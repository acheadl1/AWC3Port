/* --------
    About 2
-------------*/

.about2 {
    background: #F1FF9E;
    padding: 0 15px 0 15px;
    transition: all 1s ease;
    -webkit-transition: all 1s ease;
    text-align: center;
}

.skill-reveal {
    transition: all .2s ease;
    -webkit-transition: all .2s ease;
    background: #2187e7;
    z-index: 30;

}

.icon-reveal {
    overflow: hidden;


}

.icon-reveal:hover i {

   margin-top:-200px;
  -webkit-transition: margin .2s;
  -moz-transition: margin .2s;
  transition: margin .2s;
    
}

.icon-reveal i {
    margin-top: 25%;
}

/*---PORTFOLIO---*/



#portfolio {}

.portfolio {
	background-color: #9ec3ff;
}

.thumb {
	display: inline-block;
	max-width: 100%;
	margin-bottom: 1rem;
	border: solid 4px transparent;
	line-height: 0;
	background-color: 000;
}

.thumb-overlay {
	color: #fffff;

}

.thumb img {
-webkit-box-shadow: 1px 2px 5px 0px rgba(0,0,0,0.80); 
box-shadow: 1px 2px 5px 0px rgba(0,0,0,0.80);
background: : black;
-webkit-transition: all .3s ease;
-moz-transition: all .3s ease;
-o-transition: all .3s ease;
transition: all .3s ease;
}


.thumb img:hover {
-webkit-box-shadow: 1px 2px 5px 0px rgba(255,255,255,0.85); 
box-shadow: 1px 2px 5px 0px rgba(255,255,255,0.85);
opacity: .5;
-webkit-filter: blur(3px);

}

.thumb .thumb-title {
position: absolute;
opacity: 0;
color: #ffffff;
margin-top: 10%;
margin-left: 11%;
-webkit-transition: all .2s ease;
-moz-transition: all .2s ease;
-o-transition: all .2s ease;
transition: all .2s ease;
}

.thumb:hover .thumb-title {
opacity: 1;
color: #ffffff;
margin-top: 5%;
margin-left: 11%;
}

.dark {
color: black;
background: black;
height: 100%;

}


.modal-window {
  position: fixed;
  background-color: rgba(0, 0, 0, 0.5);
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 999;
  opacity: 0;
  pointer-events: none;
  -webkit-transition: all 0.3s;
  -moz-transition: all 0.3s;
  transition: all 0.3s;
}

.modal-window:target {
  opacity: 1;
  pointer-events: auto;
}

.modal-close {
  color: #aaa;
  line-height: 50px;
  font-size: 80%;
  text-align: right;
  text-decoration: none;
  margin-right: 3vh;
  transition: .2s ease all;
  -webkit-transition: .2 ease all;
}

.modal-close:hover {
  color: #000;
}

.modal-content {
	background-color: #ffffff;
	height: 70vh;
	width: 70vw;
	margin: auto;


}

.modal-content .fa-chevron-left {
  color: #3498db;
}

.modalbg1 {
    background: url('../projects/kentFoundation.jpg') no-repeat 0px;
    background-size: auto auto;
    overflow: hidden;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    animation: modalslide 20s infinite;
    animation-fill-mode: forwards;
    opacity: 1;
}

.modalbg2 {
    background: url('../projects/kentPortal.jpg') no-repeat 0px;
    background-size: auto auto;
    overflow: hidden;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    animation: modalslide 20s infinite;
    animation-fill-mode: forwards;
    opacity: 1;
}

.modalbg3 {
    background: url('../projects/kentAuth.jpg') no-repeat 0px;
    background-size: auto auto;
    overflow: hidden;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    animation: modalslide 20s infinite ease;
    opacity: 1;
}

.modalbg4 {
    background: url('../projects/kentDrupal.jpg') no-repeat 0px;
    background-size: auto auto;
    overflow: hidden;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    animation: modalslide 20s infinite ease;
    opacity: 1;
}

.modalbg5 {
    background: url('../projects/awc3GFX.jpg') no-repeat 0px;
    background-size: auto auto;
    overflow: hidden;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    animation: modalslide 20s infinite ease;
    opacity: 1;
}

.modal-content-container {
  padding: 5vh;
	height: 90%;

}

 @keyframes modalslide {

  0% {
    background-position: 0px;
  }

  50% {
    background-position: -350px;

  }

  100% {
    background-position: 0px;
  }

 }



/*--- CONTACT ---*/



#footer {
height: 15vh;
z-index: 10;
}

#footer div {
z-index: 10;

}

#footer i {
  margin-top: 2vh;
  margin-left: 1vh;
  transition: all .1s ease;
  color: #ffffff;
  z-index: 10;
}

#footer i:hover {
  color:  #fefefe;
}

.copyright {
  letter-spacing: .1em;
  font-size: .8em;
  opacity: .7;
  color: #ffffff;
}




/* MEDIA QUERIES! ---
----------------------------------------------------------------------------------------------------*/



/* --- SMALL SCREEN! (Phones)
-------------------------------------------------- */

@media screen and (max-width: 39.9375em) {

.modal-content-container h3 {
  font-size: 2rem;
}

.modal-close {
margin-top: 10%

}

.modal-content-container .fa {
  margin-top: 3%;

}


#portfolio .btn {
  margin-top: 5vh;

}

.modal-content {
  background-color: #ffffff;
  height: 100vh;
  width: 100vw;
  margin: auto;

}


.modalbg1, .modalbg2, .modalbg3, .modalbg4, .modalbg5 {
    background-size: auto auto;
    overflow: hidden;
    -webkit-background-size: cover;
    -moz-background-size: cover;
    -o-background-size: cover;
    background-size: cover;
    opacity: .2;
    animation: modalslide 40s infinite;
    position: fixed;
    height: 90vh;
    z-index: 4;
}

.content-mobile {
  z-index: 5;
  background-color: rgb(255, 140, 0, .1);
}

.modal-close {
  color: #000;
  opacity: .7;
}


}

/* --- MEDIUM!
Tablet, laptops, & Up -------------------------------------------------- */

@media screen and (min-width: 40em) {

.modal-content-container .fa {
  margin-top: 6%;
}

.hero-content {
  left: 30%;
  top: 30%;
  backface-visibility: hidden;
}


}