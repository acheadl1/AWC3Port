/*--- GENERAL ---*/

body {
	margin: 0;
  transition: all 1s ease;
  -webkit-transition: all 1s ease;

}

body:first-child{





}

.container {
padding-right: 15px;
padding-left: 15px;
margin-left: auto;
margin-right: auto;


}

.section {
height: 100vh;
width: 100%;
display: table;
position: relative;	
}

.half-section {
height: 50vh;
width: 100%;
display: table;
position: relative;	

}

.btn {
  background: #3498db;
  border: solid 3px #3498db;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0px;
  font-family: Arial;
  color: #ffffff;
  font-size: 19px;
  text-decoration: none;
  text-align: center;
  transition: .2s ease all;
}

.btn2 {

font-family: Arial;
color: #ffffff;
font-size: 19px;
text-decoration: none;
text-align: center;
border: solid 3px #3498db;
transition: .2s ease all;

}

.btn:hover {
  background: #0069ab;
  text-decoration: none;
}

.btn2:hover {
  border: solid 3px #0069ab;

}

.section h2 {

	margin-top: 10vh;
	margin-bottom: 10vh;
}

#contact h3 {
  margin-top: 20vh;
}

/*---Nav---*/
#nav {
    bottom: 0;
    overflow: hidden;
    height: 32px;
    z-index: 3;
    width: 100%;
	 display: table;
	 position: absolute;	
}



#nav ul li {
    float:right;
    line-height:35px;
    list-style: none;
    text-transform: uppercase;
}
#nav a {
    color: white;
    display:block;
    padding: 0 20px;
    text-decoration:none;
}

#nav a:hover {
    color:gray;
}

.navTop {
	top: 0;
	position: fixed !important;
  background: #333 !important;

}

#nav .logo {
  float: left;
  line-height: 35px;
  font-family: 'Audiowide', cursive;
  text-transform: uppercase;
  color: #3498db;
  transition: 1s ease all;
}

#nav .logo:hover {
  color: #a8d6f4;
}




/* --- Hero --- */

.land-hero {
	padding-top: 25vh;
	position: relative;
	height: 100vh;
	width: 100%;
	display: table;
	position: relative;

}

.land-hero .btn, .land-hero .btn2 {
  padding: 10px 20px 10px 20px;
}

#particles-js {
  height: 100vh;
  width: 100%;
  top: 0;
  position: fixed;
}

.hero-container {
	margin-left: auto;
	margin-right: auto;
}

.land-hero h1, .land-hero h2, .land-hero h3, .land-hero h4, .land-hero h5 {
  color: #ffffff;


}

.hero-content {
position: fixed;
}



.hero-sub {
	margin-bottom: 9vh;

}

.hero-background {
	position: fixed;
	left: 0;
	z-index: -10;
	top: 0;
	width: 100%;
	height: 100%;
	background-repeat: no-repeat;
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	opacity: 1;
	background-image: url('https://static.pexels.com/photos/176851/pexels-photo-176851.jpeg');

}


